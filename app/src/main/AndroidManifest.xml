<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scarlet_scarecrow.student_app.android.jsonparse" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.scarlet_scarecrow.student_app.android.jsonparse.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.scarlet_scarecrow.student_app.android.jsonparse.parseJSON"
            android:label="@string/title_activity_parse_json" >
        </activity>
        <activity
            android:name="com.scarlet_scarecrow.student_app.android.jsonparse.Detail"
            android:label="@string/title_activity_detail" >
        </activity>
    </application>

</manifest>
